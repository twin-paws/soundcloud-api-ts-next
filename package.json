{
  "name": "soundcloud-api-ts-next",
  "version": "1.5.0",
  "description": "Next.js integration for soundcloud-api-ts â€” React hooks + secure API route handlers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/twin-paws/soundcloud-api-ts-next"
  },
  "homepage": "https://twin-paws.github.io/soundcloud-api-ts-next/",
  "keywords": [
    "soundcloud",
    "nextjs",
    "react",
    "hooks",
    "soundcloud-api",
    "typescript",
    "soundcloud-next"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/"
  },
  "dependencies": {
    "soundcloud-api-ts": "^1.9.1"
  },
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": false
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^10.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^28.0.0",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.28.16",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
